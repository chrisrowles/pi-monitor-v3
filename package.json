{
  "name": "pi-monitor",
  "version": "1.0.5",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6",
    "@fortawesome/free-brands-svg-icons": "^6.6",
    "@fortawesome/free-regular-svg-icons": "^6.6",
    "@fortawesome/free-solid-svg-icons": "^6.6",
    "@fortawesome/vue-fontawesome": "^3.0",
    "@popperjs/core": "^2.11",
    "axios": "^1.7",
    "bootstrap": "^5.3",
    "d3": "^7.9",
    "highcharts": "^11.4",
    "lodash": "^4.17",
    "pinia": "^2.1",
    "pinia-plugin-persistedstate": "^3.2.1",
    "vue": "^3.4",
    "vue-router": "^4.4",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@pinia/testing": "0.0.15",
    "@playwright/test": "^1.45",
    "@rushstack/eslint-patch": "^1.10",
    "@types/d3": "^7.4",
    "@types/jsdom": "^21.1",
    "@types/lodash": "^4.17",
    "@types/node": "^20.14",
    "@vitejs/plugin-vue": "^5.0",
    "@vue/eslint-config-prettier": "^9.0",
    "@vue/eslint-config-typescript": "^13.0",
    "@vue/test-utils": "^2.4",
    "@vue/tsconfig": "^0.5",
    "eslint": "^8.56",
    "eslint-plugin-vue": "^9.27",
    "jsdom": "^24.1",
    "npm-run-all": "^4.1",
    "prettier": "^3.3",
    "sass": "1.77.6",
    "typescript": "~5.5.3",
    "vite": "^5.3",
    "vitest": "^2.0",
    "vue-tsc": "^2.0"
  }
}
