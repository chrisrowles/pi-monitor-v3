{
  "name": "pi-monitor",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.3",
    "@fortawesome/free-brands-svg-icons": "^6.3",
    "@fortawesome/free-regular-svg-icons": "^6.3",
    "@fortawesome/free-solid-svg-icons": "^6.3",
    "@fortawesome/vue-fontawesome": "^3.0",
    "@popperjs/core": "^2.11",
    "axios": "^1.3",
    "bootstrap": "^5.2",
    "d3": "^7.8",
    "highcharts": "^10.3",
    "lodash": "^4.17",
    "pinia": "^2.0",
    "vue": "^3.2",
    "vue-router": "^4.1"
  },
  "devDependencies": {
    "@pinia/testing": "0.0.15",
    "@playwright/test": "^1.31",
    "@rushstack/eslint-patch": "^1.1",
    "@types/d3": "^7.4",
    "@types/jsdom": "^21.1",
    "@types/lodash": "^4.14",
    "@types/node": "^18.14",
    "@vitejs/plugin-vue": "^4.0",
    "@vue/eslint-config-prettier": "^7.1",
    "@vue/eslint-config-typescript": "^11.0",
    "@vue/test-utils": "^2.3",
    "@vue/tsconfig": "^0.1",
    "eslint": "^8.35",
    "eslint-plugin-vue": "^9.9",
    "jsdom": "^21.1",
    "npm-run-all": "^4.1",
    "prettier": "^2.8",
    "sass": "^1.58",
    "typescript": "~4.9.5",
    "vite": "^4.1",
    "vitest": "^0.29",
    "vue-tsc": "^1.2"
  }
}
