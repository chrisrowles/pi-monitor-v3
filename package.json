{
  "name": "system-monitor",
  "version": "1.2.0",
  "scripts": {
    "prebuild": "node -p \"'export const APP_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "dev": "vite",
    "build": "npm run prebuild && run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "prettier:fix": "prettier -w .",
    "lint": "eslint 'src/**/*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6",
    "@fortawesome/free-brands-svg-icons": "^6.6",
    "@fortawesome/free-regular-svg-icons": "^6.6",
    "@fortawesome/free-solid-svg-icons": "^6.6",
    "@fortawesome/vue-fontawesome": "^3.0",
    "@popperjs/core": "^2.11",
    "axios": "^1.7",
    "bootstrap": "^5.3",
    "d3": "^7.9",
    "highcharts": "^11.4",
    "lodash": "^4.17",
    "pinia": "^2.2",
    "pinia-plugin-persistedstate": "^4.0.1",
    "vue": "^3.5",
    "vue-router": "^4.4",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@pinia/testing": "0.1.5",
    "@playwright/test": "^1.47",
    "@rushstack/eslint-patch": "^1.10",
    "@types/d3": "^7.4",
    "@types/jsdom": "^21.1",
    "@types/lodash": "^4.17",
    "@types/node": "^22.5",
    "@vitejs/plugin-vue": "^5.1",
    "@vue/test-utils": "^2.4",
    "@vue/tsconfig": "^0.5",
    "eslint": "^9.10",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.28.0",
    "globals": "^15.9.0",
    "jsdom": "^25.0",
    "npm-run-all": "^4.1",
    "prettier": "^3.3",
    "sass": "1.78.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.5.0",
    "vite": "^5.4",
    "vitest": "^2.0",
    "vue-tsc": "^2.1"
  }
}
